{
  "name": "node-back-end-starter",
  "version": "1.0.0",
  "description": "a tool for begin a node js back-end project",
  "main": "dist/Service/index.js",
  "engines": {
    "node": "^8.9.0",
    "npm": "^5.5.1"
  },
  "keywords": [
    "node 8",
    "typescript 2.6"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virgenherrera/node-back-end-starter.git"
  },
  "contributors": [
    "Hugo Virgen <virgenherrera@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/virgenherrera/node-back-end-starter/issues"
  },
  "homepage": "https://github.com/virgenherrera/node-back-end-starter#readme",
  "scripts": {
    "lint": "node ./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json",
    "start": "run-s cleanup test build-ts serve-nodemon",
    "develop": "run-s clean build-ts serve-development",
    "serve-development": "run-p serve-nodemon watch-ts",
    "production": "run-s cleanup test build-ts serve-prod",
    "serve-prod": "forever start forever.json",
    "serve-nodemon": "nodemon -w .env -w dist/ -V",
    "serve-nodemon-debug": "nodemon -w .env -w dist/ -V --inspect",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "clean": "node ./tasks/clean.js",
    "cleanup": "run-p clean clean-logs",
    "clean-logs": "node ./tasks/cleanLogs.js",
    "provide-env": "node ./tasks/provideEnv.js",
    "provide-forever-config": "node ./tasks/provideForeverJson.js",
    "provision": "run-p provide-env provide-forever-config",
    "create-rest-handler": "node ./tasks/createRestHandler.js",
    "create-controller": "node ./tasks/createController.js",
    "create-model": "node ./tasks/createModel.js",
    "create-repository": "node ./tasks/createRepository.js",
    "create-poco": "node ./tasks/createPoco.js",
    "create-module": "node ./tasks/createModule.js"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "forever": "^0.15.3",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.1",
    "mongoose": "^4.13.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "pluralize": "^7.0.0",
    "pug": "^2.0.0-rc.4",
    "serve-favicon": "^2.4.5",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.2",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.1",
    "@types/body-parser": "^1.16.7",
    "@types/cors": "^2.8.1",
    "@types/debug": "^0.0.30",
    "@types/del": "^3.0.0",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/mongoose": "^4.7.24",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.0.47",
    "@types/pluralize": "^0.0.28",
    "@types/pug": "^2.0.4",
    "@types/serve-favicon": "^2.2.29",
    "tslint": "^5.8.0"
  }
}
