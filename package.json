{
  "name": "node-back-end-starter",
  "version": "1.0.0",
  "description": "a tool for begin a node js back-end project",
  "main": "dist/Service/index.js",
  "engines": {
    "node": "^8.9.0",
    "npm": "^5.5.1"
  },
  "keywords": [
    "node 8",
    "typescript 2.6"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virgenherrera/node-back-end-starter.git"
  },
  "contributors": [
    "Hugo Virgen <virgenherrera@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/virgenherrera/node-back-end-starter/issues"
  },
  "homepage": "https://github.com/virgenherrera/node-back-end-starter#readme",
  "dependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/body-parser": "^1.16.7",
    "@types/cors": "^2.8.1",
    "@types/debug": "^0.0.30",
    "@types/del": "^3.0.0",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.0.47",
    "@types/pug": "^2.0.4",
    "@types/request": "^2.0.7",
    "@types/serve-favicon": "^2.2.29",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.1",
    "morgan": "^1.9.0",
    "npm-run-all": "^4.1.1",
    "pug": "^2.0.0-rc.4",
    "request": "^2.83.0",
    "serve-favicon": "^2.4.5",
    "typescript": "^2.6.1"
  },
  "scripts": {
    "start": "npm-run-all -s clean build-ts serve-dev",
    "develop": "npm-run-all -p watch-ts serve-debug",
    "production": "npm-run-all -s cleanup build-ts serve-prod",
    "serve-dev": "nodemon -w .env -w dist/ --exitcrash -V",
    "serve-debug": "nodemon -w .env -w dist/ -V --inspect",
    "serve-prod": "forever start forever.json",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "clean": "node ./tasks/clean.js",
    "cleanup": "npm-run-all -p clean clean-logs",
    "clean-logs": "node ./tasks/cleanLogs.js",
    "provide-env": "node ./tasks/provideEnv.js",
    "provide-forever-config": "node ./tasks/provideForeverJson.js",
    "postinstall": "npm-run-all -p provide-env provide-forever-config"
  }
}
