{
  "name": "node-back-end-starter",
  "version": "1.0.0",
  "description": "a  helpful tool for begin a node js back-end project",
  "main": "dist/Service/index.js",
  "engines": {
    "node": "^8.9.0",
    "npm": "^5.5.1"
  },
  "keywords": [
    "node 8",
    "typescript 2.6"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virgenherrera/node-back-end-starter"
  },
  "bugs": {
    "url": "https://github.com/virgenherrera/node-back-end-starter/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/virgenherrera/node-back-end-starter#readme",
  "scripts": {
    "start": "npm-run-all -s -l cleanup lint build-ts serve-nodemon",
    "develop": "npm-run-all -s -l -c cleanup lint build-ts serve-development",
    "production": "npm-run-all -s -l cleanup lint build-ts serve-prod",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "node ./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json",
    "cleanup": "npm-run-all -p clean-transpilated clean-logs",
    "clean-logs": "node ./tasks/cleanLogs.js",
    "clean-transpilated": "node ./tasks/cleanTranspilated.js",
    "provision": "npm-run-all -p -l -c provide-env provide-forever-config provide-dbconfig",
    "provide-dbconfig": "node ./tasks/provideDbConf.js",
    "provide-env": "node ./tasks/provideEnv.js",
    "provide-forever-config": "node ./tasks/provideForeverJson.js",
    "provide-persistance": "npm-run-all -s -l -c create-database run-migrations run-seeds",
    "create-module": "node ./tasks/createModule.js",
    "create-controller": "node ./tasks/createController.js",
    "create-model": "node ./tasks/createModel.js",
    "create-poco": "node ./tasks/createPoco.js",
    "create-repository": "node ./tasks/createRepository.js",
    "create-rest-handler": "node ./tasks/createRestHandler.js",
    "create-database": "./node_modules/.bin/sequelize db:create",
    "drop-database": "./node_modules/.bin/sequelize db:drop",
    "run-migrations": "./node_modules/.bin/sequelize db:migrate",
    "revert-migrations": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "run-seeds": "./node_modules/.bin/sequelize db:seed:all",
    "revert-seeds": "./node_modules/.bin/sequelize db:seed:undo:all",
    "sequelize": "./node_modules/.bin/sequelize",
    "serve-development": "npm-run-all -p serve-nodemon-debug watch-ts",
    "serve-nodemon": "nodemon -w .env -w dist/ -V",
    "serve-nodemon-debug": "nodemon -w .env -w dist/ -V --inspect",
    "serve-prod": "forever start forever.json"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "forever": "^0.15.3",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.1",
    "mongoose": "^4.13.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "pg": "6",
    "pg-hstore": "^2.3.2",
    "pluralize": "^7.0.0",
    "pug": "^2.0.0-rc.4",
    "reflect-metadata": "^0.1.10",
    "sequelize": "^4.27.0",
    "sequelize-cli": "^3.2.0",
    "sequelize-typescript": "^0.6.1",
    "serve-favicon": "^2.4.5",
    "socket.io": "^2.0.4",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.2",
    "uuid": "^3.1.0",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/body-parser": "^1.16.7",
    "@types/cors": "^2.8.1",
    "@types/debug": "^0.0.30",
    "@types/del": "^3.0.0",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/mongoose": "^4.7.24",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.0.47",
    "@types/pluralize": "^0.0.28",
    "@types/pug": "^2.0.4",
    "@types/sequelize": "^4.0.79",
    "@types/serve-favicon": "^2.2.29",
    "@types/socket.io": "^1.4.31",
    "tslint": "^5.8.0"
  }
}
